<div class="col-md-8 offset-md-1">
     <h2>ジャンル一覧・追加</h2>
   <%= form_with model: Genre.new do |f| %>
              
      <div class="field">
   <a>ジャンル名</a>
   <%= f.string_field :name %>
   </div>
   <div>
    <a class="actions">
    <%= f.submit "新規登録" %>
    </a>
  </div>
  <% end %>
  
    
                        <tbody>
                          <th>ジャンル</th>
                            <% @genres.each do |genre| %>
                                <tr>
                                   <td>
                                    <%= genres.name %>
                                   </td>
                                  
                                     <%= f.submit "編集する" %>
                                   
                                </tr>
                            <% end %>
                        </tbody>
  </div>